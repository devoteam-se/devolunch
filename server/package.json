    "cors": "^2.8.5",
    "express": "^4.17.3",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.7",
    "pdf-parse": "1.1.1",
    "pino": "^7.10.0",
    "puppeteer": "^13.6.0",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^20.1.0",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.6.1",
    "prettier": "2.6.2",
